{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "semicolon": false,
        "no-angle-bracket-type-assertion": false,
        "prefer-const": false,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "never",
                    "arrays": "never",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ],
        "member-access": [false],
        "import-spacing": false,
        "object-literal-sort-keys": false,
        "no-console": false
    },
    "rulesDirectory": []
}
